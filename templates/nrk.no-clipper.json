{
	"schemaVersion": "0.1.0",
	"name": "nrk.no",
	"behavior": "create",
	"noteContentFormat": "# {{title}}\n\n{{selectorHtml:#innhold|remove_html:(\".lp_related\")|replace:(\"/srcset=.*1600w, /g\":\"src=\")|remove_html:(\".authors,.article-dateline,.link__container\")|markdown}}",
	"properties": [
		{
			"name": "created",
			"value": "{{time|date:\\\"YYYY-MM-DDTHH:mm:ssZ\\\"}}",
			"type": "datetime"
		},
		{
			"name": "reviewed",
			"value": "",
			"type": "datetime"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{author|split:\\\",\\\"|join}}",
			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{published|date:\\\"YYYY-MM-DD\\\"}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "webclip",
			"type": "multitext"
		},
		{
			"name": "status",
			"value": "unread",
			"type": "multitext"
		}
	],
	"triggers": [
		"https://www.nrk.no"
	],
	"noteNameFormat": "{{published|date:\"YYYY-MM-DD\"}} WEB {{title|safe_name}}",
	"path": "webclips"
}